      {
        "name": "ubuntu",
        "uri": "/v1/assemblys/assembly_factorys",
        "description": "ubuntuinstallation",
        "tags": [
          "ubuntu"
        ],
        "replicas": 5,
        "properties": {
          "domain": "megambox.com",
          "cloudsetting": "/clouds/one",
          "region": "chennai",
          "storage_type": "ssd"
        },
