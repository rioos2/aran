/healthz:
  get:
    description: Returns the health of the service
    responses:
      '200':
        description: Service is healthy
      '500':
        description: Server fault
      '503':
        description: Service temporarily unavailable
/accounts:
  post:
    description: |
      Create a new account
    body:
      application/json:
        example: |
          {
          "name": "megam",
          "email":"info",
          "first_name":"vino",
          "last_name": "v",
          "phone":"9994048897",
          "api_key": "1234567890",
          "password": "vino123",
          "states":"safa",
          "approval":"zfdgdg",
          "suspend":"true",
          "registration_ip_address": ""
          }
    responses:
      201:
        description: |
          Succesfully created a new account
        body:
          example: |
            {
            "id":"815717538049581056",
            "email":"info",
            "name":"megam",
            "token":"JDbYVymDVcy2Hkrw10",
            "api_key":"1234567890",
            "flags":0
            }
  '/{id}':
    get:
      description: |
        Retrieve account by id
      responses:
